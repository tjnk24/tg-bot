{
  "name": "nestjs-boilerplate",
  "version": "0.0.2",
  "description": "nestjs-boilerplate",
  "author": "tjnk24",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "lint": "npm-run-all lint:*",
    "lint:tsc": "npx tsc -p tsconfig.json --skipLibCheck",
    "lint:src": "npx eslint --ext=.ts ./",
    "postinstall": "npx husky install",
    "test": "npm-run-all lint",
    "build-apidocs": "npm run clean & npx @openapitools/openapi-generator-cli generate & cd apiDocs & npm publish",
    "clean": "rm -r ./apiDocs",
    "precommit": "uver && git add -A"
  },
  "dependencies": {
    "@nestjs/common": "10.4.3",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.4.3",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-express": "10.4.3",
    "@nestjs/sequelize": "10.0.1",
    "@nestjs/swagger": "7.4.2",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.6",
    "dotenv": "16.4.5",
    "express-fingerprint": "1.2.2",
    "lodash": "4.17.21",
    "pg": "8.13.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "sequelize": "6.37.3",
    "yaml": "2.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.5",
    "@nestjs/schematics": "10.1.4",
    "@openapitools/openapi-generator-cli": "2.13.7",
    "@stylistic/eslint-plugin": "2.8.0",
    "@types/bcryptjs": "2.4.6",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "4.17.21",
    "@types/lodash": "4.17.7",
    "@types/node": "22.5.5",
    "@typescript-eslint/eslint-plugin": "8.6.0",
    "@typescript-eslint/parser": "8.6.0",
    "eslint": "8.57.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-eslint-plugin": "6.2.0",
    "eslint-plugin-import": "2.30.0",
    "husky": "9.1.6",
    "npm-run-all": "4.1.5",
    "source-map-support": "0.5.21",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.2",
    "uver-cli": "1.1.0"
  }
}
